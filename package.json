{
  "name": "nuxt-leaflet-draw",
  "version": "0.0.43",
  "description": "Nuxt module for leafletjs",
  "license": "MIT",
  "contributors": [
    "Alessandro Marocchini"
  ],
  "main": "lib/module.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marketingpmi/nuxt-leaflet-draw.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "nuxt test/fixture",
    "lint": "eslint lib test",
    "test": "npm run lint && jest",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "eslintIgnore": [
    "lib/templates/*.*"
  ],
  "files": [
    "lib",
    "*.d.ts"
  ],
  "types": [
    "index.d.ts"
  ],
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true
  },
  "dependencies": {
    "@types/leaflet": "1.7.6",
    "leaflet": "^1.7.1",
    "npm-publish-git-tag": "^3.0.3",
    "vue2-leaflet": "^2.7.1",
    "vue2-leaflet-draw-toolbar": "^0.1.1"
  },
  "devDependencies": {
    "codecov": "3.7.1",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^25.1.0",
    "jsdom": "^16.4.0",
    "nuxt": "^2.14.9",
    "standard-version": "^8.0.1"
  },
  "bugs": {
    "url": "https://github.com/schlunsen/nuxt-leaflet/issues"
  },
  "homepage": "https://github.com/schlunsen/nuxt-leaflet#readme",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  },
  "author": "Alessandro Marocchini"
}
